{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "collect": {
      "cache": false
    },
    "analyze": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
